<%- include('../partials/header') %>

<h2>Flight List</h2>
<table>
  <thead>
    <tr>
      
      <th>Flight Number</th>
      
    </tr>
  </thead>
  <tbody>
    <!-- Write the line of EJS to iterate over movies using forEach -->
    
    <% flights.forEach(f=> { %>
      
  
      <tr>
      
        <td><%= f.flightNo %></td>
       
        <td><a href="/flights/<%= f._id %>">Details</a></td>
      </tr>
    <!-- Close the forEach using EJS here -->
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>